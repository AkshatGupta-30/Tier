{
  "manifest_version": 3,
  "name": "Tier",
  "version": "1.0.0",
  "description": "Tier",
  "permissions": ["bookmarks", "storage", "topSites"],
  "host_permissions": [
    "https://suggestqueries.google.com/*",
    "https://api.bing.com/*",
    "https://duckduckgo.com/*",
    "https://search.yahoo.com/*"
  ],
  "icons": {
    "16": "images/tier-16.png",
    "48": "images/tier-48.png",
    "128": "images/tier-128.png",
    "256": "images/tier-256.png",
    "512": "images/tier.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
